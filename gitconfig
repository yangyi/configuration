[core]
    safecrlf = warn
	editor = mate -w
[color]
    ui = auto
[merge]
    summary = true
[alias]
    b      = branch
    co     = checkout
    ci     = commit
    d      = diff -b
    dc     = diff -b --cached
    ds     = diff -b --stat
    l      = log -p
    s      = status
    st     = status
    # summary of what you're going to push
    ps     = log --pretty=oneline origin..master
    # like "git log", but include the diffs
    w      = whatchanged -p
    # changes since we last did a push
    wo     = whatchanged -p origin..
